{
  "cli": {
    "version": ">= 16.28.0",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.19.4"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "resourceClass": "medium"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "resourceClass": "medium"
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "resourceClass": "medium"
    }
  },
  "submit": {
    "production": {}
  }
}
